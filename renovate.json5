{
  "extends": [
    "config:base",
    "github>aquaproj/aqua-renovate-config#1.5.2",
  ],
  "automerge": true,
  "packageRules": [
    {
      "matchUpdateTypes": ["major"],
      "automerge": false
    },
    {
      "matchUpdateTypes": ["digest"],
      "enabled": false
    },
    {
      "postUpdateOptions": ["gomodTidy"],
      "matchManagers": ["gomod"]
    }
  ]
}
